{
  "name": "kasyrooms",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOST=127.0.0.1 tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production HOST=127.0.0.1 node dist/index.js",
    "start:lan": "cross-env NODE_ENV=production HOST=0.0.0.0 node dist/index.js",
    "start:port": "cross-env NODE_ENV=production HOST=127.0.0.1 PORT=5000 node dist/index.js",
    "smoke": "tsx tests/smoke.ts",
    "smoke:checklist": "tsx tests/checklist-smoke.ts",
    "test": "tsx tests/integrity.ts",
    "test:images": "tsx tests/image-proxy.ts",
    "test:images:nested": "tsx tests/image-proxy-nested.ts",
    "selftest:images": "tsx tests/selftest-images.ts",
    "test:models:proxied": "tsx tests/models-images-proxied.ts",
    "test:images:security": "tsx tests/image-proxy-security.ts",
    "test:rate-limit": "tsx tests/rate-limit-smoke.ts",
    "test:rate-limit:embedded": "tsx tests/rate-limit-embedded.ts",
    "smoke:sirplay": "tsx tests/sirplay-smoke.ts",
    "smoke:sirplay:outbound": "tsx server/scripts/smoke-sirplay-outbound.ts",
    "test:sirplay:adapter:outbound": "tsx tests/sirplay-adapter-outbound.ts",
    "test:sirplay:db": "tsx tests/sirplay-adapter-db.ts",
    "test:b2b": "tsx tests/b2b-login-tokens.ts",
    "postman:local": "newman run tests/postman/Kasyrooms_Local.postman_collection.json -e tests/postman/Local.postman_environment.json --insecure",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:restart": "pm2 restart kasyrooms",
    "test:demo:ps": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File scripts/test-demo.ps1",
    "check": "tsc -b",
    "check:clean": "tsc -b --clean",
    "check:b2b:prod": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File scripts/check-b2b-prod.ps1",
    "db:check-schema": "tsx server/scripts/check-neon-schema.ts",
    "db:ensure:sirplay": "tsx server/scripts/ensure-sirplay-schema.ts",
    "db:push": "drizzle-kit push",
    "postinstall": "npx --yes update-browserslist-db@latest || true"
  },
